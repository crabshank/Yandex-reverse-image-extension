{
  "manifest_version": 3,
  "name": "Yandex reverse image",
  "version": "2021.6",
        "content_scripts": [
    {
	      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
   "background": {
 "service_worker": "background.js"
  },
     "permissions": ["contextMenus"],
	   "host_permissions": ["<all_urls>"]
}
